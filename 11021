#include <iostream>
using namespace std;

int main(){
    int T;
    int A, B;
    int array[T-1];
    
    cin >> T;
    for (int i=0; T > i; i++){
        cin >> A >> B ;
        array[i] = A + B;   
    }
    
    for (int i=0; T > i; i++){
        cout << "Case #"<< i+1 << ": " << array[i] << endl;
    }
}
