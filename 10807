#include <iostream>
using namespace std;

int main(){
    int a, b;
    int count;
    int array[a];                 //배열선언을 처음에 반복문 안에 넣어서 틀림
    
    cin >> a ;
    for (int i=0; a > i; ++i){    //int a; a>0; a++로 써서 틀림
        cin >> array[i];        
    }
    cin >> b;
    for (int i=0; a > i; ++i){
        if ( array[i] == b){
            count++;
        }
    }
    cout << count ;
    
}
