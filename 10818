#include <iostream>
using namespace std;

int main(){
    int N;
    int max, min = 0;
    
    cin >> N;
    int array[N];
    
    for (int i=0; N > i; i++){
        cin >> array[i] ; 
    }
    
    max = array[0] ;
    for (int i=0; N > i; i++){
        if ( array[i] > max ){
            max = array[i];
        }
    }
    min = array[0] ;
    for (int i=0; N > i; i++){
        if ( array[i] < min ){
            min = array[i];
        }
    }
    
    cout << min << " " << max ;
}
