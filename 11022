#include <iostream>
using namespace std;

int main(){
    int T, A, B;
    cin >> T;
    int a[T], b[T], s[T];
    
    for(int i=0; T > i; i++){
        cin >> A >> B;
        a[i] = A ;
        b[i] = B ;
        s[i] = A + B ;
    }
    
    for(int i=0; T > i; i++){
        cout << "Case #" << i+1 << ": " << a[i] << " + " << b[i] << " = " << s[i] << endl;
    }
}
