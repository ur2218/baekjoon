#include <iostream>
using namespace std;

int main(){
    int score;
    cin >> score;                  // c++에서는 (90 <= score <= 100)와 같은 표현 사용불가
    if (score >= 90 && score <= 100){      
        cout << "A" << endl;
    }
    else if (score >= 80 && score <= 89){
        cout << "B" << endl;
    }
    else if (score >= 70 && score <= 79){
        cout << "C" << endl;
    }
    else if (score >= 60 && score <= 69){
        cout << "D" << endl;
    }
    else {                         // 마지막 else구문에서는 조건이 없으므로 ()를 생략
        cout << "F" << endl;
    }
    
    
}
